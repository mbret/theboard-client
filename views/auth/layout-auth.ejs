<!DOCTYPE html>
<html>
  <head>

      <meta charset="utf-8">
      <meta name="viewport" content="width=device-width, initial-scale=1.0">
      <title><%= title %></title>

      <!--STYLES-->
      <link rel="stylesheet" href="/styles/importer.css">
      <!--STYLES END-->

  </head>

<body class="gray-bg">

    <% if ( typeof errors != 'undefined' && errors) {%>
        <% errors.forEach(function (error) { %>
            <div class="hidden notification" data-type="error"><%= __(error) %></div>
        <% }) %>
    <% } %>
    <% if ( typeof successes != 'undefined' && successes) {%>
        <% successes.forEach(function (success) { %>
            <div class="hidden notification" data-type="success"><%= __(success) %></div>
        <% }) %>
    <% } %>

    <%- body %>

    <script src="<%= routes.configurationJS %>"></script>
    
    <!--SCRIPTS-->
    <script src="/js/dependencies/sails.io.js"></script>
    <script src="/js/dependencies/jquery/jquery-2.1.1.min.js"></script>
    <script src="/js/dependencies/jquery/jquery-ui.min.js"></script>
    <script src="/js/dependencies/bootstrap/bootstrap.min.js"></script>
    <script src="/js/dependencies/jquery-plugins/iCheck/icheck.min.js"></script>
    <script src="/js/dependencies/jquery-plugins/toastr/toastr.min.js"></script>
    <script src="/js/dependencies/jquery-plugins/validate/jquery.validate.min.js"></script>
    <script src="/js/script.auth.js"></script>
    <!--SCRIPTS END-->

</body>

</html>
